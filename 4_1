#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char *tab(char *word, int length){

    int *mass = (int*) malloc(length * sizeof(int));
    for(int i=1; i < length; i++ ){

            mass[length-i-1] = i;
            for(int j = length-i; j < length-1; j++) {
                if (word[length-i-1] == word[j] && i != j && i>1) {
                    mass[length-i-1] = mass[length-i];
                }
            }
    }
    for(int i=0; i<length-1; i++){
        if(word[i]==word[length-1]) {
            mass[length-1]=mass[i];
            break;
        }
        else mass[length-1] = mass[0]+1;
    }
//    for(int i=0; i<length; i++){
//        printf("%d ", mass[i]);
//   }
}

int main(){

    char sentense[]="kakayata yeres + slowo";
    char word[]= "slowo";
    int length = strlen(word);
    char *tablew = tab(word, length);


}
